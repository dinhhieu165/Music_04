<tr>
  <td><%= lyric.id %></td>
  <td><%= lyric.user.name %></td>
  <td><%= lyric.song.name %></td>
  <td><%= lyric.accepted %></td>
  <td><%= lyric.created_at %></td>
  <td>
    <%= link_to edit_admin_lyric_path(category) do %>
      <span><i class="fa fa-edit fa-1px"></i></span>
    <% end %>
  </td>
  <td>
    <%= link_to admin_lyric_path(category), method: :delete, data: {confirm: "Are you sure?"} do %>
      <span><i class="fa fa-times fa-1px"></i></span>
    <% end %>
  </td>
</tr>s
