<%= form_for [:admin, @artist] do |f| %>
  <%= render "shared/error_messages", object: f.object %>
    <div class="field">
      <%= f.label :name, t("admin.artists.name") %>
      <%= f.text_field :name, class: "form-control" %>
    </div>
    <div class="field">
      <%= f.label :describe, t("admin.artists.describe") %>
      <%= f.text_field :describe, class: "form-control"%>
    </div>
    <div class="field">
      <%= f.submit :Submit, class: "btn btn-primary" %>
    </div>
<% end %>
